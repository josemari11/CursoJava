package poo1;

public class Coche {
	private int ruedas;
	private int largo;
	private int ancho;
	private int motor;
	private int peso;
	
	//crear métodos
	public Coche(){
		ruedas=4;
		largo=2000;
		ancho=300;
		motor=1600;
		peso=1000;
	}
	public String dimeLargo(){
		
		return "el largo del coche es "+ largo;
	}

}//class coche