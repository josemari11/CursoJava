package poo1;

public class Coche {
	int ruedas;
	int largo;
	int ancho;
	int motor;
	int peso;
	
	//crear métodos
	public Coche(){
		ruedas=4;
		largo=2000;
		ancho=300;
		motor=1600;
		peso=1000;
	}

public static void main(String args[]){
	Coche c1=new Coche();//Instanciar una clase,ejemplar de clase Coche
	System.out.println("este coche tiene "+ c1.ruedas+ " ruedas");
	}
}